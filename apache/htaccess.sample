# This file needs to be copied next to the compiled index.html
# Replace FRONT_HREF variable with front url suffix, for instance /geoshop2

<IfModule mod_rewrite.c>
    RewriteEngine on
	RewriteBase ${FRONT_HREF}
    RewriteCond %{REQUEST_FILENAME} -s [OR]
    RewriteCond %{REQUEST_FILENAME} -l [OR]
    RewriteCond %{REQUEST_FILENAME} -d
    RewriteRule ^.*$ - [NC,L]
    RewriteRule ^(.*) ${FRONT_HREF} [NC,L]
</IfModule>
